{
  "name": "ca.karrotmarket.com",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/websites.git",
    "directory": "ca.karrotmarket.com"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "scripts": {
    "prepare-gatsby": "babel gatsby -d . -x .ts,.tsx --presets gatsby-package",
    "postinstall": "yarn prepare-gatsby",
    "develop": "yarn prepare-gatsby && gatsby develop",
    "build": "yarn prepare-gatsby && gatsby build  --prefix-paths",
    "clean": "gatsby clean",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@karrotmarket/gatsby-theme-global-website": "workspace:^",
    "gatsby": "4.10.2",
    "gatsby-plugin-google-gtag": "4.10.0",
    "react": "next",
    "react-dom": "next"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/plugin-transform-typescript": "7.16.8",
    "babel-plugin-polished": "1.1.0",
    "babel-preset-gatsby-package": "2.10.0",
    "typescript": "4.6.2"
  }
}
