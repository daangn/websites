  },
  "devDependencies": {
    "@babel/cli": "7.18.9",
    "@babel/core": "7.18.9",
    "@types/node": "18.0.0",
    "babel-preset-gatsby-package": "2.18.0",
    "concurrently": "7.2.2",
    "gatsby": "4.18.0",
    "typescript": "4.7.4"
  }
}
