{
  "name": "@karrotmarket/gatsby-source-greenhouse-job-board",
  "version": "0.0.1",
  "description": "Gatsby source plugin for sourcing content from Greenhouse Job Board API",
  "main": "./index.js",
  "scripts": {
    "build": "concurrently \"yarn:build:*\"",
    "build:babel": "babel src -d . -x .ts",
    "build:dts": "tsc --emitDeclarationOnly --skipLibCheck",
    "prepack": "yarn build"
  },
  "files": [
    "src",
    "index.js",
    "utils.js",
    "gatsby-node.js",
    "types.d.ts",
    "utils.d.ts"
  ],
  "peerDependencies": {
    "gatsby": "^2.2.0 || ^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "got": "^11.8.2",
    "undici": "^4.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@types/node": "^14.14.37",
    "babel-preset-gatsby-package": "^1.5.0",
    "concurrently": "^6.0.2",
    "gatsby": "^3.5.0",
    "typescript": "^4.2.4"
  }
}
