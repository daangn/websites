{
  "name": "@karrotmarket/gatsby-source-greenhouse-job-board",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Gatsby source plugin for sourcing content from Greenhouse Job Board API",
  "main": "./index.js",
  "scripts": {
    "build": "concurrently \"yarn:build:*\"",
    "build:babel": "babel gatsby -d . -x .ts --presets gatsby-package",
    "build:dts": "tsc --emitDeclarationOnly --skipLibCheck",
    "prepack": "yarn build"
  },
  "files": [
    "src",
    "index.js",
    "utils.js",
    "gatsby-node.js",
    "types.d.ts",
    "utils.d.ts",
    "LICENSE"
  ],
  "peerDependencies": {
    "gatsby": "^2.2.0 || ^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "got": "^11.8.2"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/plugin-transform-typescript": "7.14.6",
    "@types/node": "14.17.7",
    "babel-preset-gatsby-package": "1.10.0",
    "concurrently": "6.2.0",
    "gatsby": "next",
    "typescript": "4.3.5"
  }
}
